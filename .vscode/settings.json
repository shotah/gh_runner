{
    "telemetry.enableTelemetry": false,
    "editor.formatOnSave": true,
    "editor.defaultFormatter": "ms-python.black-formatter",
    "[python]": {
      "editor.defaultFormatter": "ms-python.black-formatter",
      "editor.formatOnSave": true,
      "editor.codeActionsOnSave": {
        "source.organizeImports": "explicit"
      }
    },
    "[markdown]": {
      "editor.tabSize": 2,
      "editor.defaultFormatter": "DavidAnson.vscode-markdownlint"
    },
    "[makefile]": {
      "editor.formatOnSave": false,
      "editor.insertSpaces": false,
      "editor.tabSize": 4,
      "editor.detectIndentation": false
    },
    "[json]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[yaml]": {
      "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "files.eol": "\n",
    "files.autoSave": "off",
    "files.trimFinalNewlines": true,
    "files.associations": {},
    "python.testing.pytestEnabled": true,
    "python.testing.unittestEnabled": false,
    "python.linting.enabled": true,
    "python.linting.flake8Enabled": true,
    "python.formatting.provider": "none",
    "isort.check": true,
    "cSpell.words": [
      "addopts",
      "AKIAIOSFODNN",
      "apigw",
      "asciidoc",
      "autopep",
      "boto",
      "celerybeat",
      "conftest",
      "Connor",
      "dmypy",
      "dotenv",
      "eventbridge",
      "exitfirst",
      "FASTAPI",
      "flake",
      "functools",
      "getenv",
      "htmlcov",
      "ipynb",
      "ipython",
      "isort",
      "jmespath",
      "KHTML",
      "lambdas",
      "Logformat",
      "millis",
      "mkdocs",
      "moto",
      "myapp",
      "mypy",
      "nosetests",
      "onehundred",
      "pipenv",
      "Pipfile",
      "powertools",
      "pycache",
      "pydantic",
      "pyenv",
      "pyflow",
      "pypa",
      "pypackages",
      "pypi",
      "pyproject",
      "pytest",
      "pythonpath",
      "pyupgrade",
      "Redrive",
      "ropeproject",
      "samconfig",
      "scrapy",
      "sdist",
      "Spyder",
      "spyderproject",
      "spyproject",
      "testpaths",
      "toml",
      "venv",
      "VIRTUALENVS",
      "webassets",
      "wsgi"
    ]
  }
  